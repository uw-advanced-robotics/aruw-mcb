        "aruw-mcb-project/taproot/src",
        "aruw-mcb-project/src",
        "aruw-mcb-project/taproot/modm/ext/cmsis/dsp",
        "aruw-mcb-project/taproot/modm/ext/cmsis/core"
    ],
    "C_Cpp.default.cppStandard": "c++20",
    "C_Cpp.default.cStandard": "c11",
    "C_Cpp.default.forcedInclude": [
        "aruw-mcb-project/robot-type/robot_type.hpp",
        ".vscode/intellisense_config.h"
    ],
    "C_Cpp.default.browse.limitSymbolsToIncludedHeaders": true,
    "[markdown]": {
        "editor.wordWrap": "off",
        "editor.quickSuggestions": {
            "comments": "off",
            "strings": "off",
            "other": "off"
        },
        "editor.rulers": [
            100
        ]
    },
    "git.autoRepositoryDetection": false,
    // For file templates (see extensions section in README.md)
    "auto-snippets.snippets": [
        {
            "pattern": "**/*_command.hpp",
            "snippet": "declare-command"
        },
        {
            "pattern": "**/*_subsystem.hpp",
            "snippet": "declare-subsystem"
        },
    ]
